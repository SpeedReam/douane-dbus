<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/org/zedroot/Douane">
  <interface name="org.zedroot.Douane">
  	<!-- Properties -->
    <property name="DaemonVersion" type="s" access="read"/>
    <!-- Methods -->
    <method name="RegisterAsDialogProcess">
      <arg type="s" name="process_id" direction="out"/>
    </method>
    <method name="UnregisterDialogProcess">
      <arg type="s" name="process_id" direction="in"/>
    </method>
    <method name="GetRules">
      <arg type="a(ssb)" name="rules" direction="out"/>
    </method>
    <method name="DeleteRule">
      <arg type="s" name="rule_id" direction="in"/>
      <arg type="b" name="result" direction="out"/>
    </method>
    <!-- Signals -->
    <signal name="NewActivityToBeValidated">
      <arg type="(ssss)" name="activity"/>
    </signal>
  </interface>
</node>
